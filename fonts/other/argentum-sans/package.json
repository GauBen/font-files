{
  "name": "@fontsource/argentum-sans",
  "version": "5.2.4",
  "description": "Self-host the Argentum Sans font in a neatly bundled NPM package.",
  "main": "index.css",
  "publishConfig": {"access": "public"},
  "keywords": [
    "fontsource",
    "font",
    "font family",
    "google fonts",
    "argentum-sans",
    "Argentum Sans",
    "css",
    "sass",
    "front-end",
    "web",
    "typeface",
    "variable"
  ],
  "exports": {
    ".": {"sass": "./index.css", "default": "./index.css"},
    "./*": {"sass": "./*.css", "default": "./*.css"},
    "./files/*": {"sass": "./files/*", "default": "./files/*"},
    "./package.json": "./package.json",
    "./metadata.json": "./metadata.json",
    "./unicode.json": "./unicode.json",
    "./scss": {"sass": "./scss/metadata.scss"}
  },
  "author": "The Argentum Sans Project Authors (cssobral2013@gmail.com - https://github.com/cssobral2013/Argentum-Sans)",
  "license": "OFL-1.1",
  "homepage": "https://fontsource.org/fonts/argentum-sans",
  "funding": "https://github.com/sponsors/ayuhito",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fontsource/font-files.git",
    "directory": "fonts/other/argentum-sans"
  },
  "publishHash": "e8827b12c40a0457"
}